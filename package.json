{
  "name": "imagio",
  "version": "1.0.0",
  "description": "Tauri-based OCR and Image Generation App for macOS",
  "keywords": [
    "tauri",
    "ocr",
    "tesseract",
    "macos"
  ],
  "homepage": "https://github.com/gy-0/Imagio#readme",
  "bugs": {
    "url": "https://github.com/gy-0/Imagio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gy-0/Imagio.git"
  },
  "license": "MIT",
  "author": "Yuan Gao",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix && prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:dev:fast": "CARGO_PROFILE_DEV_OPT_LEVEL=2 tauri dev",
    "tauri:build": "tauri build && npm run post-build && npm run copy-app",
    "post-build": "cd src-tauri && ./add-icon-name.sh target/release/bundle/macos/Imagio.app",
    "copy-app": "rm -rf /Applications/Imagio.app && cp -r src-tauri/target/release/bundle/macos/Imagio.app /Applications/ && echo 'âœ… Imagio.app installed to /Applications/'"
  },
  "dependencies": {
    "@google/genai": "^1.25.0",
    "@tauri-apps/api": "^2.2.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-http": "^2.5.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@tauri-apps/cli": "^2.8.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.1.9",
    "vitest": "^4.0.6"
  }
}
