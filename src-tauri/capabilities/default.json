{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "core:path:default",
    "core:image:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://*.bfl.ai/*" }
      ]
    }
  ]
}
